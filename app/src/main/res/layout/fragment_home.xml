<LinearLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nuc.jingbeibei.studentdailymanagement.ui.home.HomeFragment"
    android:orientation="vertical">
    <com.nuc.jingbeibei.studentdailymanagement.ui.home.ImageSlideshow.ImageSlideshow
        android:id="@+id/is_gallery"
        android:layout_width="match_parent"
        android:layout_height="192dp">

    </com.nuc.jingbeibei.studentdailymanagement.ui.home.ImageSlideshow.ImageSlideshow>
<Button
    android:id="@+id/id_class_management_btn"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="老师班级管理"
    />
    <Button
        android:id="@+id/id_class_counselor_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我是辅导员"
        />
    <Button
        android:id="@+id/id_notice_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="公告"
        />
    <Button
        android:id="@+id/id_address_book_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="通讯录"
        />

</LinearLayout>
